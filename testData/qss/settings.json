[
    {

        "path": "more",
        "schema": {
            "type": "more",
            "title": "More"
        },
        "tooltip": {
            "header": "More options for changing the settings on this computer."
        }
    }, {
        "path": "http://registry\\.gpii\\.net/common/highContrastTheme",
        "schema": {
            "type": "string",
            "title": "Hi / Lo Contrast",
            "enum": [
                "Black on brown",
                "Grey on white",
                "Grey on black",
                "Black on yellow",
                "Yellow on black",
                "White on black",
                "Black on white",
                "Regular contrast"
            ],
            "defaultValue": "Regular contrast"
        },
        "value": "Regular contrast",
        "styles": {
            "Black on brown": {
                "background-color": "#bb9966",
                "color": "#000000",
                "border-color": "transparent"
            },
            "Grey on white": {
                "background-color": "#ffffff",
                "color": "#6c6c6c",
                "border-color": "#6c6c6c"
            },
            "Grey on black": {
                "background-color": "#222222",
                "color": "#888888",
                "border-color": "transparent"
            },
            "Black on yellow": {
                "background-color": "#ffff00",
                "color": "#000000",
                "border": "2px dashed #6c6c6c"
            },
            "Yellow on black": {
                "background-color": "#222222",
                "color": "#ffff00",
                "border-color": "transparent"
            },
            "White on black": {
                "background-color": "#222222",
                "color": "#ffffff",
                "border-color": "transparent"
            },
            "Black on white": {
                "background-color": "#ffffff",
                "color": "#000000",
                "border-color": "#656565"
            },
            "Regular contrast": {
                "background": "#ffffff",
                "color": "#0073bd",
                "border-color": "#0073bd"
            }
        },
        "tooltip": {
            "header": "Change the contrast between the text and background color, so text is easier to read.",
            "body": "Contrast changes work in Windows, Edge, and some other applications."
        }
    }, {
        "path": "http://registry\\.gpii\\.net/common/selfVoicing/enabled",
        "schema": {
            "title": "Read Aloud",
            "description": "Whether to enable/disable self voicing",
            "type": "boolean",
            "defaultValue": false
        },
        "value": false,
        "tooltip": {
            "header": "Turn Chrome’s ability to read text on/off."
        }
    }, {   
        "path": "http://registry\\.gpii\\.net/common/captions/enabled",
        "schema": {
            "type": "boolean",
            "title": "Captions",
            "defaultValue": false
        },
        "value": false,
        "tooltip": {
            "header": "Turn captions on/off in",
            "body": "<ul><li> Youtube videos played in Chrome</li><li>Windows Media Player </li></ul>"
        }
    }, {
        "path": "http://registry\\.gpii\\.net/common/DPIScale",
        "schema": {
            "type": "number",
            "title": "Screen Size",
            "min": 1,
            "max": 2,
            "divisibleBy": 0.25,
            "defaultValue": 1
        },
        "value": 1.25,
        "tooltip": {
            "header": "Change the screen size to make everything larger/smaller."
        }
    }, {
        "path": "http://registry\\.gpii\\.net/common/fontSize",
        "schema": {
            "type": "number",
            "title": "Text Size",
            "min": 10,
            "max": 13,
            "divisibleBy": 0.5,
            "defaultValue": 11
        },
        "value": 11,
        "tooltip": {
            "header": "Change text size.",
            "body": "Works in Windows, Edge, Chrome, Firefox, MIcrosoft Office, and many other applications."
        }
    }, {
        "path": "http://registry\\.gpii\\.net/common/language",
        "schema": {
            "type": "string",
            "title": "Language",
            "enum": [
                "English",
                "Deutsch",
                "Español",
                "Български"
            ],
            "defaultValue": "English"
        },
        "value": "English",
        "tooltip": {
            "header": "Change language.",
            "body": "Works in Windows, Edge, and many other applications."
        }
    }, {
        "path": "save",
        "schema": {
            "type": "save",
            "title": "Save"
        },
        "tooltip": {
            "header": "Save your preferences so you can access them from other Morphic-enabled computers."
        }
    }, {
        "path": "undo",
        "schema": {
            "type": "undo",
            "title": "Undo"
        },
        "tooltip": {
            "header": "Undo the last change you made in  the Morphic Bar."
        }
    }, {
        "path": "keyIn",
        "schema": {
            "type": "keyIn",
            "image": "gpii-icon-balloon.png"
        },
        "tooltip": {
            "header": "Open the Morphic Panel.",
            "body": "Where you can use your Morphic account to adjust your personal settings on this computer."
        }
    }, {
        "path": "close",
        "schema": {
            "type": "close",
            "title": "Close"
        },
        "tooltip": {
            "header": "Close the Morphic Panel.",
            "body": "You can re-open it from the system tray or from the Morphic desktop icon."
        }
    }
]
