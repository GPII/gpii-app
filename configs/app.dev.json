{
    "type": "gpii.appDevWithTaskTray",
    "options": {
        "gradeNames": ["fluid.component"],
        "distributeOptions": {
            "distributeTaskTray": {
                "record": "gpii.appWrapper",
                "target": "{that flowManager}.options.gradeNames"
            },
            "distributeMenuDisplay": {
                "record": "gpii.app.menuInAppDev",
                "target": "{that menu}.options.gradeNames"
            },
            "distributeDevPcpChannelConnector": {
                "record": "gpii.app.dev.gpiiConnector",
                "target": "{that gpiiConnector}.options.gradeNames"
            },
            "distributePcpChannelConnector": {
                "record": {
                    "hostname": "localhost",
                    "port": 8081,
                    "path": "/pcpChannel"
                },
                "target": "{that gpiiConnector}.options.config"
            },
            "distributeSurveyConnector": {
                "record": "gpii.app.staticSurveyConnector",
                "target": "{that surveyConnector}.options.gradeNames"
            },
            "distributeStaticSurveyConnectorFixture": {
                "record": "%gpii-app/testData/survey/triggers.dev.json",
                "target": "{that staticSurveyConnector}.options.paths.triggerFixture"
            }
        }
    },
    "mergeConfigs": [
        "%gpii-universal/gpii/configs/gpii.config.untrusted.development.all.local.json5"
     ]
}
