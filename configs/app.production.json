{
    "type": "gpii.appWithTaskTray",
    "options": {
        "gradeNames": ["fluid.component"],
        "distributeOptions": {
            "distributeTaskTray": {
                "record": "gpii.appWrapper",
                "target": "{that flowManager}.options.gradeNames"
            },
            "distributePcpChannelConnector": {
                "record": {
                    "hostname": "localhost",
                    "port": 8081,
                    "path": "/pcpChannel"
                },
                "target": "{that gpiiConnector}.options.config"
            },
            "distributeSurveyConnector": {
                "record": {
                    "hostname": "astea@surveyserver.gpii.net",
                    "port": 33334
                },
                "target": "{that surveyConnector}.options.config"
            }
        }
    },
    "mergeConfigs": [
        "../node_modules/universal/gpii/configs/gpii.config.all.development.dr.production.json"
     ]
}
